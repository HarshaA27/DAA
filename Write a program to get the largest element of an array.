def find_largest_element(arr):
    if len(arr) == 0:
        return None 
    largest = arr[0]  
    for num in arr:
        if num > largest:
            largest = num  
    return largest
array = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
largest_element = find_largest_element(array)
print(f"The largest element in the array is {largest_element}.")
