def fibonacci(n):
    if n <= 0:
        return "Input should be a positive integer."
    elif n == 1:
        return 0
    elif n == 2:
        return 1
    else:
        return fibonacci(n-1) + fibonacci(n-2)
def print_fibonacci_series(num_terms):
    for i in range(1, num_terms + 1):
        print(fibonacci(i), end=" ")
num_terms = 10
print_fibonacci_series(num_terms)
